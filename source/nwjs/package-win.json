{
	"main": "index.html",
	"name": "Linum Framework",
	"description": "[[[description]]]",
	"version": "[[[version]]]",
	"window": {
		"icon": "icon-128.png",
		"width": [[[width]]],
		"height": [[[height]]],
		"position": "center",
		"resizable": [[[resizable]]],
		"frame": [[[frame]]],
		"kiosk": [[[kiosk]]],
		"show": false
	},
	"dom_storage_quota": 250,
	"user-agent": "Linum Framework, NWjs/%nwver",
	"chromium-args": "--disable-plugins --disable-internal-flash --disable-popup-blocking --enable-gamepad --enable-html5-camera --allow-file-access-from-files --ignore-gpu-blacklist --force-color-profile srgb  --limit-fps=60 --js-flags=--expose-gc --user-data-dir=browser_userdata"
}